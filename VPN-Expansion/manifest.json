{
  "manifest_version": 2,
  "name": "VPN Barbaris",
  "version": "1.0",
  "description": "Быстрый и безопасный VPN",
  "icons": {
    "16": "assets/Icon16Off.png",
    "32": "assets/Icon32Off.png"
  },
  "permissions": [
    "proxy",
    "storage",
    "webRequest",
    "webRequestAuthProvider",
    "notifications"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "assets/Icon16Off.png",
      "32": "assets/Icon32Off.png"
    }
  }
}